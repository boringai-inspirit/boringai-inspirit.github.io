---
import favIcon from '~/assets/favicons/logo.png';
import favIconSvg from '~/assets/favicons/logo.png';
import appleTouchIcon from '~/assets/favicons/logo.png';
---

<link rel="shortcut icon" href={favIcon.src} />
<link rel="icon" type="image/png" href={favIconSvg.src} />
<link rel="mask-icon" href={favIconSvg.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
